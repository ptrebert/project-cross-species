{
  "version": 1,
  "formatters": {
    "short": {
      "format": "[%(levelname)s] [%(asctime)s]: %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "long": {
      "format": "[%(levelname)s] [%(asctime)s] [%(funcName)s]->[%(lineno)s]: %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "normal": {
      "class": "logging.StreamHandler",
      "formatter": "short",
      "level": "WARNING",
      "stream": "ext://sys.stderr"
    },
    "debug": {
      "class": "logging.StreamHandler",
      "formatter": "long",
      "level": "DEBUG",
      "stream": "ext://sys.stderr"
    }
  },
  "loggers": {
    "default": {
      "handlers": ["normal"],
      "level": "WARNING",
      "propagate": "True"
    },
    "debug": {
      "handlers": ["debug"],
      "level": "DEBUG",
      "propagate": "True"
    }
  },
  "disable_existing_loggers": "True"
}
